<!--pages/schedule/schedule.wxml-->

<view class="table" bindtouchstart="handleTouchStart" bindtouchmove="handleTouchMove" bindtouchend="handleTouchEnd">
  <view class="tr bg-w">
    <view class="th"></view>
    <view class="th">日</view>
    <view class="th">一</view>
    <view class="th">二</view>
    <view class="th">三</view>
    <view class="th">四</view>
    <view class="th">五</view>
    <view class="th">六</view>
  </view>
  <block wx:for="{{listData}}" wx:key="{{code}}">
    <view class="tr" id="{{index}}">
      <view class="td" id="0">{{item[0]}}</view>
      <view class="{{item[1] ? 'td choose-g' : 'td'}}" ></view>
      <view class="{{item[2] ? 'td choose-g' : 'td'}}" ></view>
      <view class="{{item[3] ? 'td choose-g' : 'td'}}" ></view>
      <view class="{{item[4] ? 'td choose-g' : 'td'}}" ></view>
      <view class="{{item[5] ? 'td choose-g' : 'td'}}" ></view>
      <view class="{{item[6] ? 'td choose-g' : 'td'}}" ></view>
      <view class="{{item[7] ? 'td choose-g' : 'td'}}" ></view>
    </view>
  </block>
</view>
